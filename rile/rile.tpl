<canvas class="rilejs-spinner" width="45" height="45" ></canvas>
<div class="rilejs-toolbar">
	<button title="Previous page" data-action="prevPage"><span class="icon-arrow-left"></span></button>
	<button title="Next page" data-action="nextPage"><span class="icon-arrow-right"></span></button>
	<button title="Fullscreen" data-action="toggleFullScreen"><span class="icon-fullscreen"></span></button>
	<button title="Settings" data-action="openSettings"><span class="icon-cog"></span></button>
</div>

<div class="rilejs-settings">
<div class="rilejs-settings-font-size">
	<span class="icon-font large" data-value="large"></span>
	<span class="icon-font medium" data-value="medium"></span>
	<span class="icon-font small" data-value="small"></span>
</div>
<div class="rilejs-settings-colors">
	<span class="icon-adjust light" data-value="light"></span>
	<span class="icon-adjust gray" data-value="gray"></span>
	<span class="icon-adjust sepia" data-value="sepia"></span>
	<span class="icon-adjust dark" data-value="dark"></span>
	<span class="icon-adjust black" data-value="black"></span>
	<span class="icon-adjust black2" data-value="black2"></span>
</div>
<div class="rilejs-tip"></div>
</div>

<div class="rilejs-title"></div>
<div class="rilejs-progress"><div><span></span></div></div>

<div class="rilejs-container">

<div class="rilejs-page1-container">
	<iframe class="rilejs-page1" class="rilejs-page" scrolling="no"></iframe>
	<span class="rilejs-page-number"></span>
</div>
<div class="rilejs-renderer-container">
	<iframe class="rilejs-renderer" class="rilejs-page" scrolling="no"></iframe>
</div>

<div class="rilejs-prev-page">
	<span class="icon-arrow-left"></span>
</div>
<div class="rilejs-next-page">
	<span class="icon-arrow-right"></span>
</div>
